<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Mqtt_act">

    <TextView
        android:id="@+id/textView7"
        android:layout_width="272dp"
        android:layout_height="53dp"
        android:text="MQTT BROKER"
        android:textSize="36sp"
        android:textStyle="normal|bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.611"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.126" />

    <LinearLayout
        android:layout_width="304dp"
        android:layout_height="331dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <Button
            android:id="@+id/button8"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:onClick="libro1"
            android:text="Farenheith" />

        <Button
            android:id="@+id/button9"
            android:layout_width="match_parent"
            android:layout_height="58dp"
            android:onClick="revival"
            android:text="Revival" />

        <Button
            android:id="@+id/button10"
            android:layout_width="match_parent"
            android:layout_height="62dp"
            android:onClick="libro3"
            android:text="El Alquimista" />

        <Button
            android:id="@+id/button11"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:onClick="libro4"
            android:text="El Poder" />

        <Button
            android:id="@+id/button12"
            android:layout_width="match_parent"
            android:layout_height="59dp"
            android:onClick="libro5"
            android:text="Despertar" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>